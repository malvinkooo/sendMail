{
    parser: 'babel-eslint',
    extends: 'airbnb',
    plugins: [
        'react'
    ],
    rules: {
        linebreak-style: 0,
        'indent': 0,
        'react/jsx-indent': ['error', 4],
        'react/prop-types': 0,
        'no-underscore-dangle': 0,
    },
    globals: {
        'window': true,
    }
}